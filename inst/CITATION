citHeader("To cite spatialEco in publications use:")

spatialEco.ref <- c(
   bibentry(
     bibtype = "Manual",
     title = "spatialEco",
     author = person(c("Jeffrey", "S."), "Evans", role = c("aut", "cre")),
     year = "2014",
     note = "R package version 0.1-0",
     url = "http://CRAN.R-project.org/package=spatialEco",
     key = "spatialEco-package"
   )
)

