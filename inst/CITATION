citHeader("To cite spatialEco in publications use:")

spatialEco.ref <- c(
   bibentry(
     bibtype = "Manual",
     title = "spatialEco",
     author = person(c("Jeffrey", "S."), "Evans", role = c("aut", "cre")),
     year = "2019",
     note = "R package version 1.2-0",
     url = "https://github.com/jeffreyevans/spatialEco",
     key = "spatialEco-package"
   )
)
